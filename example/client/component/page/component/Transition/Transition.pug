include ../mixin.pug

div.component-transition
  router-view

  +section('transitionProps', '过渡动画公共的 props')
      z-table(
          border='row'
          auto
          :pageSize='10')
        template(
            slot='thead'
            v-for='item in ["名字", "类型", "可选值", "说明"]')
          z-table-col {{ item }}
        z-table-row(slot='1')
          z-table-col speed
          z-table-col string
          z-table-col slow | *normal | fast
          z-table-col 过渡的速度